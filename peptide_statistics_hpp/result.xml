    <view id="view_stats" label="View Stats">
        <blockRef id="main" type="view_stats"/>
    </view>
    <block id="view_stats" type="table_ss">
        <data>
            <source type="file" name="library_coverage_summary_statistics/"/>
            <parsers>
                <parser type="sortedTabular" sortBy="category" operator="ascending"/>
            </parsers>
        </data>
        <row>
            <column type="text" field="category" label="category" width="20"/>
            <column type="text" field="value" label="value" width="20"/>
        </row>
    </block>
</interface>
